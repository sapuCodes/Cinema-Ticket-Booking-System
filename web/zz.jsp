<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>JSP Page</title>
    </head>
    <body>
        watch trailer button
        <button id="watch-trailer-btn" style="background: transparent; border: 2px solid #fff;
                    color: #fff; padding: 10px 20px; font-size: 1.1rem; font-weight: 600; border-radius: 30px;
                    cursor: pointer; margin: 10px; transition: background-color 0.3s ease, color 0.3s ease;" 
              onmouseover="this.style.backgroundColor='#fff'; this.style.color='#000';" 
              onmouseout="this.style.backgroundColor='transparent'; this.style.color='#fff';">
              Watch Trailer
            </button>
    </body>
</html>
